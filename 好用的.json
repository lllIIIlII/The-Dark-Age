{
	"type": "RadialEffect",
	"amount": 10
	"effect":"dynamicSpikes",
	"rotationSpacing":10
	"lengthOffset":8
	"startDelay":60
	},

"startDelay":
//触发延迟
"region":
//自定义形状，需用"模组名字-贴图名字"
"spin":
//特效旋转速度
"instBomb"
//圆圈星，就是雷霆的轰炸子弹落地的特效，可直接使用
"interp": "fastSlow",
//缓速
"interp": "swing",
//波纹
"interp": "bounceIn",
//弹力球
"sprite": "circle-bullet",
//子弹圆球
"interp": "pow10Out",
"sizeInterp": "pow5In",
//烟尘
"interp": "pow3In",
"sizeInterp": "pow5Out",
//吸
"randLength": false,
//特效随机位移
"type":"hitEmpSpark"
//工厂生产东西的特效
"despawnEffect":
//子弹消失的特效。与hitEffect不同，此特效在子弹消失的地方生成特效，hitEffect命中实体后在命中处生成特效。若子弹命中，不触发despawn，触发hit。当攻击处于分裂系时，不命中hitEffect就不会触发。用不到的话最好关掉
"shootEffect": "instShoot",
//恶兆发射
"parts":[
//几何形状，不会消失的特效
"collidesAir":false,
//对空碰撞
"collidesGround":false,
//对地碰撞
"absorbable":false,
//立场墙碰撞
"reflectable":false,
//布墙反弹
"hittable":false,
//点防分解
"collides": false,
//碰撞体积
"collidesTiles": false,
//碰撞敌方
"collideTerrain":false
//碰撞墙体，是背景墙，不是造出来的墙
"collidesTeam": false
//碰撞己方
"pierceBuilding": true,
//穿透建筑，穿透一个建筑造成一次伤害
"pierce": true
//穿透单位
"instantDisappear": true,
//瞬间消失，跟 lifetime=0 差不多。不过相比于用lifetime=0，它可以使炮台预判单位位置
"splashDamagePierce": true,
//爆炸穿透，对建筑伤害=splashDamage×size^2，对单位不明显  要是不想被喷就不要填这玩意
"drag":
// 总位移 = speed * [1 - (1-drag)^lifetime] / drag
"weaveScale": 5,//波长
"weaveMag": 2,//振幅

"type": "PointBulletType",//点击式触发器，特殊情况下可用
"speed": 0,
"damage": 1000,
"lifetime":0,
"despawnEffect": "none"
"collidesGround":false,
"collidesAir":false,
"absorbable":false,
"reflectable":false,
"hittable":false,
"collides": false,
"collidesTiles": false,
"hitSound":"none",
"hitEffect": "none",
"trailEffect": "none"
//触发器，json堆堆乐用

